{
  "openapi": "3.0.1",
  "info": {
    "title": "E-Commerce API",
    "description": "",
    "version": "1.0.0"
  },
  "tags": [
    {
      "name": "Admin - Dashboard"
    }
  ],
  "paths": {
    "/admin/dashboard/sales": {
      "get": {
        "summary": "Get sales data",
        "tags": [
          "Admin - Dashboard"
        ],
        "security": [
          {
            "bearer": []
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "amount": {
                      "type": "number"
                    },
                    "number_of_sales": {
                      "type": "number"
                    }
                  }
                },
                "example": {
                  "amount": 1000,
                  "number_of_sales": 10
                }
              }
            }
          }
        }
      }
    },
    "/admin/dashboard/user": {
      "get": {
        "summary": "Get user data",
        "tags": [
          "Admin - Dashboard"
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "user_count": {
                      "type": "number"
                    },
                    "average_value_per_user": {
                      "type": "number"
                    }
                  }
                },
                "example": {
                  "user_count": 100,
                  "average_value_per_user": 10
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "bearer": {
        "type": "http",
        "scheme": "bearer"
      }
    }
  },
  "servers": [
    {
      "url": "/api/v1"
    }
  ]
}
